            "language": "javascript",
            "source": [
                "function equalizeArray(arr) {\n    // Write your code here\n    let count=1;\n    arr.sort();\n\n    let max= 1;\n    \n  for(let i=0;i<arr.length; i++ ){\n      \n        if(arr[i] === arr[i+1]){\n            count++;\n      \n            if(count>max)\n            max=count\n        }\n        \n        else{\n             count=1\n             }\n       \n        \n  }\n  \nreturn(arr.length - max)\n    \n  \n\n}"
            ],
